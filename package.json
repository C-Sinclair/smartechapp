{
  "name": "smartechapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "storyserver": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "compile": "tsc --noEmit -p . --pretty",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-circular-slider": "^1.0.1",
    "react-native-dial": "^1.0.12",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-screens": "^2.0.0-alpha.15",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-sound": "^0.11.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.13.3",
    "rn-sprite-sheet": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-notes": "^5.2.6",
    "@storybook/addon-ondevice-knobs": "^5.2.6",
    "@storybook/addon-ondevice-notes": "^5.2.6",
    "@storybook/addon-storyshots": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/react-native": "^5.2.6",
    "@storybook/react-native-server": "^5.2.6",
    "@storybook/theming": "^5.2.6",
    "@types/detox": "^14.5.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jasmine": "^3.5.0",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.13",
    "@types/react-native": "^0.57.65",
    "@types/react-native-snap-carousel": "^3.7.4",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "detox": "^14.7.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-extended": "^0.11.2",
    "jest-fetch-mock": "^2.1.2",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^16.5.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react-devtools-core": "^4.2.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.12.0",
    "react-native-config": "^0.12.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "16.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/(?!setup).*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "e2e/*"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "./__tests__/setup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native).+\\.js$"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./App"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyloader.js"
    }
  }
}
